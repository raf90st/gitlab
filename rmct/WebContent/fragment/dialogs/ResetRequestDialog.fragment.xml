<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog id="ResetRequestDialogId" title="Reset Request" afterClose="onResetClose" contentWidth="30%">
		<beginButton>
			<Button text="Close" press="onGenericFragmentclose($event, '_resetRequestDialog')"></Button>
		</beginButton>
		<endButton>
			<Button text="Reset" press="onRequestReset"></Button>
		</endButton>
	<core:Fragment
		fragmentName="juliusbaer.rmct.fragment.inputs.ResetRequestInput" type="XML" />
	</Dialog>
</core:FragmentDefinition>