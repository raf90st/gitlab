<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog title="Non-Financial Goal" contentWidth="30%">
		<beginButton>
			<Button text="Close" press="onGenericFragmentclose($event, '_nonFinancialGoalDialog')"></Button>
		</beginButton>
		<endButton>
			<Button text="Save" enabled="{global>/rmFieldStates/nfgSaveButtonEnabled}" press="onSaveNonFinancialGoal"></Button>
		</endButton>
	<core:Fragment
		fragmentName="juliusbaer.rmct.fragment.inputs.NonFinancialGoalInput" type="XML" />
	</Dialog>
</core:FragmentDefinition>