<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog id="newMdRegionDialog" title="Add Region" afterClose=""
		contentWidth="30%">
		<beginButton>
			<Button text="Cancel"
				press="onGenericFragmentclose($event, '_newMdRegionDialog')"></Button>
		</beginButton>
		<endButton>
			<Button text="Save" press="onSaveNewMdRegion"></Button>
		</endButton>
		<HBox>
			<Label text="Key:"></Label>
		</HBox>
		<HBox>
			<Input type="Text" value="{newMdRegion>/Key}" />
		</HBox>
		<HBox>
			<Label text="Text:"></Label>
		</HBox>
		<HBox>
			<Input type="Text" value="{newMdRegion>/Text}" />
		</HBox>
		<HBox>
			<Label text="Default:"></Label>
			<CheckBox selected="{newMdRegion>/Valid}" />
		</HBox>
	</Dialog>
</core:FragmentDefinition>