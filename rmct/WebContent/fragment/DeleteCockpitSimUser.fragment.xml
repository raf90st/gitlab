<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog id="confirmDeleteUserRole" title="Delete User Role" afterClose="dialogAfterclose">
		<Text text="Are you sure you want to delete selected user role(s)? "></Text>
		<beginButton>
			<Button text="Cancel" press=".onCancelDeleteNewUserRole"></Button>
		</beginButton>
		<endButton>
			<Button text="Confirm" press=".onConfirmDeleteUserRole"></Button>
		</endButton>
	</Dialog>
</core:FragmentDefinition>